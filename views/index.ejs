<!DOCTYPE html>
<html lang="en">

  <%- include('partials/head') %>

<body>

  <!-- ***** Preloader Start ***** -->
  <%- include('partials/preloader') %>
  
  <!-- ***** Preloader End ***** -->

  <!-- ***** Header Area Start ***** -->
  <%- include('partials/header') %>
  <script></script>
  <!-- ***** Header Area End ***** -->

  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="page-content">

          <!-- ***** Banner Start ***** -->
          <div class="main-banner">
            <div class="row">
              <div class="col-lg-7">
                <div class="header-text">
                  <h6>Welcome To My Portfolio</h6>
                  <h4><em style="font-size: 50px; color: white;">Aymane EL MANDILI</em> <br> <em style="font-size: 24px;">computer science engineer</em> </h4>
                  <div class="main-button">
                    <a href="browse.html">Browse Now</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <!-- ***** Banner End ***** -->

          <!-- ***** Most Popular Start ***** -->
          
          <!-- ***** Most Popular End ***** -->

          <!-- ***** Gaming Library Start ***** -->
          <div class="gaming-library">
            <div class="col-lg-12">
              <div class="heading-section">
                <h4><em>Study</em> Experience</h4>
              </div>
              
              <% for(let i =0 ; i < studies.length; i++) { %>
                <div class="item">
                  <ul>
                    <li><img src=<%=studies[i].img%> alt="" class="templatemo-item"></li>
                    <li><h4> <%= studies[i].abreviation %> </h4><span> <%= studies[i].institue %> </span></li>
                    <li><h4>Major</h4><span> <%= studies[i].title %>  </span></li>
                    <li><h4>Dates</h4><span> <%= studies[i].dateStarted %> </span><br><span> <%= studies[i].dateEnded %> </span></li>
                    <!-- <li><h4>Currently</h4><span>Downloaded</span></li> -->
                    <!-- <li><div class="main-border-button border-no-active"><a href="#">Donwloaded</a></div></li> -->
                  </ul>
                </div>
              <% } %>

              
            </div>
            <div class="col-lg-12">
              <div class="main-button">
                <a href="profile.html">View Your Library</a>
              </div>
            </div>
          </div>
          <!-- ***** Gaming Library End ***** -->
        </div>
      </div>
    </div>
  </div>
  
 

  <%- include('partials/footer') %> 
  <!-- Scripts -->
  <!-- Bootstrap core JavaScript -->
  <%- include('partials/bootstrap-scripts') %>


  </body>

</html>
